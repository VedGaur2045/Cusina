<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:background="@drawable/bg_black_border_border_nine"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/_7sdp"
    android:paddingBottom="@dimen/_7sdp"
    android:paddingStart="@dimen/_10sdp"
    android:paddingEnd="@dimen/_10sdp"
    android:layout_marginStart="@dimen/_7sdp"
    android:layout_marginEnd="@dimen/_7sdp">

    <TextView
        android:id="@+id/timeStartTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/startingTime"
        android:textColor="@color/_000000"
        android:fontFamily="@font/sourcesanspro_regular"
        android:textSize="@dimen/_10sdp"
        android:layout_centerVertical="true"/>

    <TextView
        android:layout_toEndOf="@+id/timeStartTxt"
        android:id="@+id/dashTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dashIcon"
        android:textColor="@color/_000000"
        android:fontFamily="@font/sourcesanspro_bold"
        android:textSize="@dimen/_10sdp"
        android:layout_centerVertical="true"/>

    <TextView
        android:layout_toEndOf="@+id/dashTxt"
        android:id="@+id/timeEndTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/endTime"
        android:textColor="@color/_000000"
        android:fontFamily="@font/sourcesanspro_regular"
        android:textSize="@dimen/_10sdp"
        android:layout_centerVertical="true"/>

</RelativeLayout>