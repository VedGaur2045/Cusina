<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:fillViewport="true"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/_30sdp">

            <RelativeLayout
                android:id="@+id/mainLayout"
                android:layout_margin="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent">

                <LinearLayout
                    android:id="@+id/headerLayout"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageButton
                        android:id="@+id/backButton"
                        android:background="@color/transparent"
                        android:src="@mipmap/back"
                        android:layout_gravity="center"
                        android:layout_width="@dimen/_24sdp"
                        android:layout_height="@dimen/_24sdp"/>

                    <TextView
                        android:layout_marginStart="@dimen/_15sdp"
                        android:id="@+id/orderTxt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/orderTxt"
                        android:textColor="@color/_000000"
                        android:textSize="@dimen/_24ssp"
                        fonthPath="fonts/cabin_semibold.otf"
                        tools:ignore="MissingPrefix"
                        android:layout_marginLeft="@dimen/_15sdp" />

                    <TextView
                        android:id="@+id/orderNumber"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/_41"
                        android:textColor="@color/_000000"
                        android:textSize="@dimen/_24ssp"
                        fonthPath="fonts/cabin_semibold.otf"
                        tools:ignore="MissingPrefix"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_below="@+id/headerLayout"
                    android:id="@+id/processingImageLayout"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20sdp">

                    <ImageView
                        android:id="@+id/ellipse_1"
                        android:layout_width="@dimen/_16sdp"
                        android:layout_height="@dimen/_16sdp"
                        android:src="@mipmap/ellipse_4"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginLeft="@dimen/_5sdp" />

                    <ImageView
                        android:id="@+id/line_1"
                        android:layout_width="@dimen/_66sdp"
                        android:layout_height="@dimen/_16sdp"
                        android:src="@mipmap/line_3"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginEnd="@dimen/_7sdp"/>

                    <ImageView
                        android:id="@+id/ellipse_2"
                        android:layout_width="@dimen/_16sdp"
                        android:layout_height="@dimen/_16sdp"
                        android:src="@mipmap/ellipse_4"/>

                    <ImageView
                        android:id="@+id/line_2"
                        android:layout_width="@dimen/_66sdp"
                        android:layout_height="@dimen/_16sdp"
                        android:src="@mipmap/line_3"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginEnd="@dimen/_7sdp"/>

                    <ImageView
                        android:id="@+id/ellipse_3"
                        android:layout_width="@dimen/_16sdp"
                        android:layout_height="@dimen/_16sdp"
                        android:src="@mipmap/ellipse_4"/>

                    <ImageView
                        android:id="@+id/line_3"
                        android:layout_width="@dimen/_66sdp"
                        android:layout_height="@dimen/_16sdp"
                        android:src="@mipmap/line_3"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_marginEnd="@dimen/_7sdp"/>

                    <ImageView
                        android:id="@+id/ellipse_4"
                        android:layout_width="@dimen/_16sdp"
                        android:layout_height="@dimen/_16sdp"
                        android:src="@mipmap/ellipse_4"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_below="@+id/processingImageLayout"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:id="@+id/processingTextLayout"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_5sdp">

                    <TextView
                        android:id="@+id/ConfirmOrder"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="wrap_content"
                        android:text="@string/ConfirmOrder"
                        android:textSize="@dimen/_8ssp"
                        android:textColor="@color/_000000"
                        tools:ignore="MissingPrefix"
                        fonthPath="fonts/sourcesanspro_regular.otf"/>

                    <TextView
                        android:id="@+id/Preparing"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Preparing"
                        android:textSize="@dimen/_8ssp"
                        android:textColor="@color/_000000"
                        fonthPath="fonts/sourcesanspro_regular.otf"
                        tools:ignore="MissingPrefix"
                        android:layout_marginStart="@dimen/_50sdp"
                        android:layout_marginLeft="@dimen/_50sdp" />

                    <TextView
                        android:id="@+id/Deliver"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginStart="@dimen/_60sdp"
                        android:layout_marginLeft="@dimen/_60sdp"
                        fonthPath="fonts/sourcesanspro_regular.otf"
                        tools:ignore="MissingPrefix"
                        android:text="@string/Deliver"
                        android:textColor="@color/_000000"
                        android:textSize="@dimen/_8ssp" />

                    <TextView
                        android:id="@+id/Complete"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Complete"
                        android:textSize="@dimen/_8ssp"
                        android:textColor="@color/_000000"
                        tools:ignore="MissingPrefix"
                        fonthPath="fonts/sourcesanspro_regular.otf"
                        android:layout_marginStart="@dimen/_75sdp"
                        android:layout_marginLeft="@dimen/_75sdp"/>

                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/orderTimeLayout"
                    android:layout_below="@+id/processingTextLayout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_35sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:background="@drawable/black_bg_border_nine"
                    android:translationZ="@dimen/_6sdp"
                    android:translationY="@dimen/_3sdp">

                    <TextView
                        android:id="@+id/ORDER_TIME_TXT"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/orderTime"
                        android:textSize="@dimen/_14ssp"
                        android:textColor="@color/_FF8500"
                        android:layout_marginStart="@dimen/_17sdp"
                        android:layout_centerVertical="true"
                        fonthPath="fonts/sourcesanspro_semibold.otf"
                        tools:ignore="MissingPrefix"
                        android:layout_marginLeft="@dimen/_17sdp"
                        android:shadowColor="@color/_7F000000"
                        android:shadowDx="1"
                        android:shadowDy="1"
                        android:shadowRadius="2"/>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="@dimen/_17sdp"
                        android:layout_marginRight="@dimen/_15sdp">

                        <TextView
                            android:id="@+id/hourTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/_00"
                            android:textSize="@dimen/_14ssp"
                            android:textColor="@color/_FF8500"
                            fonthPath="fonts/sourcesanspro_semibold.otf"
                            tools:ignore="MissingPrefix"
                            android:shadowColor="@color/_7F000000"
                            android:shadowDx="1"
                            android:shadowDy="1"
                            android:shadowRadius="2"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/_colonIcon"
                            android:textSize="@dimen/_14ssp"
                            android:textColor="@color/_FF8500"
                            fonthPath="fonts/sourcesanspro_semibold.otf"
                            tools:ignore="MissingPrefix"
                            android:shadowColor="@color/_7F000000"
                            android:shadowDx="1"
                            android:shadowDy="1"
                            android:shadowRadius="2"/>

                        <TextView
                            android:id="@+id/minuteTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/_03"
                            android:textSize="@dimen/_14ssp"
                            android:textColor="@color/_FF8500"
                            fonthPath="fonts/sourcesanspro_semibold.otf"
                            tools:ignore="MissingPrefix"
                            android:shadowColor="@color/_7F000000"
                            android:shadowDx="1"
                            android:shadowDy="1"
                            android:shadowRadius="2"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/_colonIcon"
                            android:textSize="@dimen/_14ssp"
                            android:textColor="@color/_FF8500"
                            fonthPath="fonts/sourcesanspro_semibold.otf"
                            tools:ignore="MissingPrefix"
                            android:shadowColor="@color/_7F000000"
                            android:shadowDx="1"
                            android:shadowDy="1"
                            android:shadowRadius="2"/>

                        <TextView
                            android:id="@+id/secondTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/_56"
                            android:textSize="@dimen/_14ssp"
                            android:textColor="@color/_FF8500"
                            fonthPath="fonts/sourcesanspro_semibold.otf"
                            tools:ignore="MissingPrefix"
                            android:shadowColor="@color/_7F000000"
                            android:shadowDx="1"
                            android:shadowDy="1"
                            android:shadowRadius="2"/>

                    </LinearLayout>

                </RelativeLayout>

                <TextView
                    android:layout_marginTop="@dimen/_8sdp"
                    android:layout_below="@+id/orderTimeLayout"
                    android:id="@+id/customerNameTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_CustomerName"
                    android:textSize="@dimen/_16ssp"
                    android:textColor="@color/_000000"
                    fonthPath="fonts/cabin_semibold.otf"
                    tools:ignore="MissingPrefix"/>

                <TextView
                    android:layout_marginTop="@dimen/_8sdp"
                    android:layout_below="@+id/customerNameTxt"
                    android:id="@+id/customerName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_productName"
                    android:textSize="@dimen/_24ssp"
                    android:textColor="@color/_000000"
                    fonthPath="fonts/cabin_semibold.otf"
                    tools:ignore="MissingPrefix"/>

                <RelativeLayout
                    android:layout_below="@+id/customerNameTxt"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true">

                    <ImageButton
                        android:background="@color/transparent"
                        android:id="@+id/callImgBtn"
                        android:layout_width="@dimen/_16sdp"
                        android:layout_height="@dimen/_16sdp"
                        android:src="@mipmap/call_img"/>

                    <ImageButton
                        android:background="@color/transparent"
                        android:id="@+id/messageImgBtn"
                        android:layout_width="@dimen/_16sdp"
                        android:layout_height="@dimen/_16sdp"
                        android:src="@mipmap/message_img"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:layout_toEndOf="@+id/callImgBtn"
                        android:layout_toRightOf="@+id/callImgBtn"
                        android:layout_marginLeft="@dimen/_7sdp" />

                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/emailLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/customerName"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_marginTop="@dimen/_8sdp"
                        android:id="@+id/customerEmailTxt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/_EmailAddress"
                        android:textSize="@dimen/_12ssp"
                        android:textColor="@color/_000000"
                        tools:ignore="MissingPrefix"
                        fonthPath="fonts/sourcesanspro_regular.otf" />

                    <TextView
                        android:layout_marginTop="@dimen/_8sdp"
                        android:id="@+id/customerEmail"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/_CustomerEmailTxt"
                        android:textSize="@dimen/_12ssp"
                        android:textColor="@color/_000000"
                        android:layout_marginStart="@dimen/_15sdp"
                        tools:ignore="MissingPrefix"
                        fonthPath="fonts/sourcesanspro_regular.otf"
                        android:layout_marginLeft="@dimen/_15sdp" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/phoneLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/emailLayout"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_marginTop="@dimen/_8sdp"
                        android:id="@+id/customerPhoneTxt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/_PhoneNumber"
                        android:textSize="@dimen/_12ssp"
                        android:textColor="@color/_000000"
                        fonthPath="fonts/sourcesanspro_regular.otf"
                        tools:ignore="MissingPrefix"/>

                    <TextView
                        android:layout_marginTop="@dimen/_8sdp"
                        android:id="@+id/customerPhone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/_CustomerPhoneTxt"
                        android:textSize="@dimen/_12ssp"
                        android:textColor="@color/_000000"
                        android:layout_marginStart="@dimen/_10sdp"
                        fonthPath="fonts/sourcesanspro_regular.otf"
                        tools:ignore="MissingPrefix"
                        android:layout_marginLeft="@dimen/_10sdp"/>

                </LinearLayout>

                <TextView
                    android:layout_below="@+id/phoneLayout"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:id="@+id/customerLocationTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_Location"
                    android:textSize="@dimen/_16ssp"
                    android:textColor="@color/_000000"
                    fonthPath="fonts/cabin_semibold.otf"
                    tools:ignore="MissingPrefix"/>

                <LinearLayout
                    android:id="@+id/location"
                    android:layout_below="@+id/customerLocationTxt"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/mapImage"
                        android:layout_width="73dp"
                        android:layout_height="@dimen/_83sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:scaleType="fitXY"
                        android:src="@mipmap/map_sample" />

                    <LinearLayout
                        android:id="@+id/addressLayout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:layout_marginLeft="@dimen/_15sdp"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/firstAddress"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/firstAddress"
                            android:textColor="@color/_000000"
                            android:textSize="@dimen/_14ssp"
                            fonthPath="fonts/sourcesanspro_semibold.otf"
                            tools:ignore="MissingPrefix"/>

                        <TextView
                            android:id="@+id/secondAddress"
                            android:layout_width="@dimen/_124sdp"
                            android:layout_height="wrap_content"
                            android:text="@string/secondAddress"
                            android:textColor="@color/_000000"
                            android:textSize="@dimen/_12ssp"
                            fonthPath="fonts/sourcesanspro_regular.otf"
                            tools:ignore="MissingPrefix"/>

                        <TextView
                            android:id="@+id/addOtherAddress"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_3sdp"
                            android:text="@string/addOtherAddress"
                            android:textColor="@color/_80000000"
                            android:textSize="@dimen/_8ssp"
                            fonthPath="fonts/sourcesanspro_regular.otf"
                            tools:ignore="MissingPrefix"/>

                    </LinearLayout>

                </LinearLayout>

                <TextView
                    android:layout_below="@+id/location"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:id="@+id/Items"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Items"
                    android:textSize="@dimen/_16ssp"
                    android:textColor="@color/_000000"
                    fonthPath="fonts/cabin_semibold.otf"
                    tools:ignore="MissingPrefix"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:layout_below="@+id/Items"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/itemList"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:scrollbarThumbVertical="@android:color/transparent"
                    android:nestedScrollingEnabled="false"/>

                <RelativeLayout
                    android:layout_marginTop="@dimen/_12sdp"
                    android:id="@+id/TtotalLayout"
                    android:layout_below="@+id/itemList"
                    android:layout_width="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_alignParentEnd="true">

                    <TextView
                        android:id="@+id/totalTxt"
                        android:layout_alignParentBottom="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/total"
                        android:textColor="@color/_000000"
                        android:textSize="@dimen/_12ssp"
                        fonthPath="fonts/sourcesanspro_semibold.otf"
                        tools:ignore="MissingPrefix"/>

                    <TextView
                        android:id="@+id/phpTxt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/_pHp"
                        android:textColor="@color/_000000"
                        android:textSize="@dimen/_24ssp"
                        fonthPath="fonts/sourcesanspro_semibold.otf"
                        tools:ignore="MissingPrefix"
                        android:layout_toEndOf="@+id/totalTxt"
                        android:layout_marginStart="@dimen/_10sdp"
                        android:layout_toRightOf="@+id/totalTxt"
                        android:layout_marginLeft="@dimen/_10sdp" />

                    <TextView
                        android:id="@+id/phpAmount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/_600"
                        android:textColor="@color/_000000"
                        android:textSize="@dimen/_24ssp"
                        fonthPath="fonts/sourcesanspro_semibold.otf"
                        tools:ignore="MissingPrefix"
                        android:layout_toEndOf="@+id/phpTxt"
                        android:layout_toRightOf="@+id/phpTxt" />

                </RelativeLayout>

            </RelativeLayout>

            <View
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:background="@color/_F6F6F6"/>

            <RelativeLayout
                android:id="@+id/secondLayout"
                android:layout_margin="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent">

                <TextView
                    android:id="@+id/deliveryTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_DeliveryMethods"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/_16ssp"
                    fonthPath="fonts/cabin_semibold.otf"
                    tools:ignore="MissingPrefix"/>

                <RadioGroup
                    android:layout_below="@+id/deliveryTxt"
                    android:id="@+id/radioGroup"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <RadioButton
                        android:id="@+id/pickUpOnly"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:button="@drawable/checkbox_selector"
                        android:text="@string/Pick_up_Only"
                        android:checked="true"
                        android:paddingLeft="@dimen/_10sdp"
                        tools:targetApi="lollipop" />

                    <RadioButton
                        android:id="@+id/delivery"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:button="@drawable/checkbox_selector"
                        android:layout_marginLeft="@dimen/_18sdp"
                        android:paddingLeft="@dimen/_10sdp"
                        android:text="@string/delivery_method2"
                        android:layout_marginStart="@dimen/_18sdp" />

                </RadioGroup>

                <TextView
                    android:id="@+id/feeTxt"
                    android:layout_below="@+id/deliveryTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/fee"
                    android:layout_gravity="center"
                    android:textSize="@dimen/_14ssp"
                    android:textColor="@color/black"
                    android:layout_marginLeft="@dimen/_25sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    fonthPath="fonts/sourcesanspro_regular.otf"
                    tools:ignore="MissingPrefix"
                    android:layout_toEndOf="@+id/radioGroup"
                    android:layout_toRightOf="@+id/radioGroup" />

                <EditText
                    android:id="@+id/feeInPHPTxt"
                    android:layout_below="@+id/deliveryTxt"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_width="@dimen/_66sdp"
                    android:layout_height="@dimen/_18sdp"
                    android:background="@drawable/gray_colo_back"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_8sdp"
                    android:hint="@string/_pHp"
                    android:inputType="number"
                    android:singleLine="true"
                    android:paddingLeft="@dimen/_10sdp"
                    android:textSize="@dimen/_10ssp"
                    fonthPath="fonts/sourcesanspro_lightit.otf"
                    tools:ignore="MissingPrefix"
                    android:textColor="@color/black"
                    android:layout_toEndOf="@+id/feeTxt"
                    android:layout_toRightOf="@+id/feeTxt" />

            </RelativeLayout>

            <View
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:background="@color/_F6F6F6"/>

            <RelativeLayout
                android:id="@+id/thirdLayout"
                android:layout_margin="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent">

                <TextView
                    android:id="@+id/orderdateTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/_orderDate"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/_16ssp"
                    fonthPath="fonts/cabin_semibold.otf"
                    tools:ignore="MissingPrefix"/>

                <TextView
                    android:layout_below="@+id/orderdateTxt"
                    android:id="@+id/weekAndDateOfOrder"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/weekAndDateOfOrder"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/_16ssp"
                    fonthPath="fonts/sourcesanspro_regular.otf"
                    tools:ignore="MissingPrefix"/>

            </RelativeLayout>

            <View
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:background="@color/_F6F6F6"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_15sdp">

                <TextView
                    android:id="@+id/expectedTimeTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/expectedTimeTxt"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/_16ssp"
                    fonthPath="fonts/cabin_semibold.otf"
                    tools:ignore="MissingPrefix"/>

                <TextView
                    android:layout_marginTop="@dimen/_7sdp"
                    android:layout_below="@+id/expectedTimeTxt"
                    android:id="@+id/expectedTimeOrder"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/expectedTimeOrder"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/_16ssp"
                    fonthPath="fonts/sourcesanspro_regular.otf"
                    tools:ignore="MissingPrefix"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/pageBtn"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_marginEnd="@dimen/_30sdp">

                <Button
                    android:id="@+id/ConfirmOrderBtn"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_37sdp"
                    android:text="@string/ConfirmOrder"
                    android:background="@drawable/redbuttonbg"
                    android:textSize="@dimen/_14ssp"
